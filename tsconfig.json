{
    "files": [],
    "references": [
      { "path": "./tsconfig.app.json" },
      { "path": "./tsconfig.node.json" }
    ],
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"],
        "@contexts/*": ["./src/contexts/*"]
      },
      "noImplicitAny": true,
      "strictNullChecks": true,
      "skipLibCheck": true,
      "allowJs": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true
    }
  }